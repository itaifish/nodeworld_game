{
	"name": "my-t3-app",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"build": "run-s build:*",
		"build:1-next": "cross-env NODE_ENV=production next build",
		"build:2-server": "tsc --project tsconfig.server.json",
		"dev:next": "next dev",
		"dev:wss": "cross-env PORT=3001 SKIP_ENV_VALIDATION=true tsx watch src/server/websockets/websocketsServer.ts --tsconfig tsconfig.server.json",
		"dev": "run-p dev:*",
		"postinstall": "prisma generate",
		"generate": "prisma generate",
		"migrate": "prisma migrate dev",
		"prisma:reset": "prisma migrate reset",
		"prisma:studio": "prisma studio",
		"lint": "next lint --fix",
		"cypress:open": "cypress open",
		"start": "cross-env NODE_ENV=production node --experimental-specifier-resolution=node dist/src/server/prodServer.js"
	},
	"dependencies": {
		"@next-auth/prisma-adapter": "^1.0.5",
		"@prisma/client": "^4.14.1",
		"@tanstack/react-query": "^4.20.0",
		"@trpc/client": "^10.27.1",
		"@trpc/next": "^10.27.1",
		"@trpc/react-query": "^10.27.1",
		"@trpc/server": "^10.27.1",
		"dotenv": "^16.0.3",
		"next": "13.1.2",
		"next-auth": "^4.18.7",
		"node-fetch": "^3.3.1",
		"phaser": "^3.55.2",
		"phaser3-rex-plugins": "^1.1.79",
		"pino": "^8.8.0",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"superjson": "1.9.1",
		"uuid": "^9.0.0",
		"ws": "^8.13.0",
		"zod": "^3.20.2"
	},
	"devDependencies": {
		"@types/node": "^18.11.18",
		"@types/react": "^18.0.26",
		"@types/react-dom": "^18.0.10",
		"@types/uuid": "^9.0.1",
		"@types/ws": "^8.5.4",
		"@typescript-eslint/eslint-plugin": "^5.49.0",
		"@typescript-eslint/parser": "^5.49.0",
		"cross-env": "^7.0.3",
		"cypress": "^12.4.0",
		"eslint": "^8.32.0",
		"eslint-config-next": "^13.1.5",
		"eslint-config-prettier": "^8.6.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-react": "^7.32.1",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.8.3",
		"prisma": "^4.14.1",
		"tsx": "^3.12.7",
		"typescript": "^5.0.4"
	},
	"ct3aMetadata": {
		"initVersion": "7.3.2"
	}
}
